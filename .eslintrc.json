{
    "extends": ["airbnb-base", "prettier"],
    "env": {
        "node": true
    },
    "parserOptions": {
        "ecmaVersion": 8
    },
    "rules": {
        "no-console": "off",
        "indent": ["error", 2],
        "semi": ["error", "never"],
        "no-return-assign": "warn",
        "no-param-reassign": "warn"
    }
}
